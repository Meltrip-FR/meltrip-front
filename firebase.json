{
  "hosting": {
    "public": "out",
    "target": "default",
    "appAssociation": "AUTO",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/500",
        "destination": "/500.html"
      },
      {
        "source": "/404",
        "destination": "/404.html"
      },
      {
        "source": "/teams",
        "destination": "/teams.html"
      },
      {
        "source": "/contact",
        "destination": "/contact.html"
      },
      {
        "source": "/cgu",
        "destination": "/cgu.html"
      },
      {
        "source": "/cgv",
        "destination": "/cgv.html"
      },
      {
        "source": "/auth/signin",
        "destination": "/auth/signin.html"
      },
      {
        "source": "/auth/signup",
        "destination": "/auth/signup.html"
      },
      {
        "source": "/auth/seminar/signup",
        "destination": "/auth/seminar/signup.html"
      },
      {
        "source": "/user/accessibility",
        "destination": "/user/accessibility.html"
      },
      {
        "source": "/user/dashboard",
        "destination": "/user/dashboard.html"
      },
      {
        "source": "/user/organization",
        "destination": "/user/organization.html"
      },
      {
        "source": "/user/partner",
        "destination": "/user/partner.html"
      },
      {
        "source": "/user/payments",
        "destination": "/user/payments.html"
      },
      {
        "source": "/user/profil",
        "destination": "/user/profil.html"
      },
      {
        "source": "/user/seminar",
        "destination": "/user/seminar.html"
      },
      {
        "source": "/user/seminar/infos/*",
        "destination": "/user/seminar/infos/[id].html"
      },
      {
        "source": "/user/seminar/infos/quotes/*",
        "destination": "/user/seminar/infos/quotes/[id].html"
      },
      {
        "source": "/invites/*",
        "destination": "/invites/[id].html"
      },
      {
        "source": "/invites/limit",
        "destination": "/invites/limit.html"
      },
      {
        "source": "/invites/thanks",
        "destination": "/invites/thanks.html"
      },
      {
        "source": "/seminar/create",
        "destination": "/seminar/create.html"
      },
      {
        "source": "/blog",
        "destination": "/blog.html"
      },
      {
        "source": "/blog/article/*",
        "destination": "/blog/article/[id].html"
      },
      {
        "source": "/blog/tag/*",
        "destination": "/blog/tag/[id].html"
      },
      {
        "source": "/confirm/*",
        "destination": "/confirm/[usertag].html"
      },
      {
        "source": "/confirm/forgot/*",
        "destination": "/confirm/forgot/[password].html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}

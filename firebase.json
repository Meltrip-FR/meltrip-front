{
  "hosting": {
    "public": "out",
    "target": "default",
    "appAssociation": "AUTO",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/500",
        "destination": "/500.html"
      },
      {
        "source": "/404",
        "destination": "/404.html"
      },
      {
        "source": "/teams",
        "destination": "/teams.html"
      },
      {
        "source": "/contact",
        "destination": "/contact.html"
      },
      {
        "source": "/help",
        "destination": "/help.html"
      },
      {
        "source": "/auth/signin",
        "destination": "/auth/signin.html"
      },
      {
        "source": "/auth/signup",
        "destination": "/auth/signup.html"
      },
      {
        "source": "/auth/seminar/signup",
        "destination": "/auth/seminar/signup.html"
      },
      {
        "source": "/user/accessibility",
        "destination": "/user/accessibility.html"
      },
      {
        "source": "/user/dashboard",
        "destination": "/user/dashboard.html"
      },
      {
        "source": "/user/organization",
        "destination": "/user/organization.html"
      },
      {
        "source": "/user/partner",
        "destination": "/user/partner.html"
      },
      {
        "source": "/user/payments",
        "destination": "/user/payments.html"
      },
      {
        "source": "/user/profil",
        "destination": "/user/profil.html"
      },
      {
        "source": "/user/seminar",
        "destination": "/user/seminar.html"
      },
      {
        "source": "/blog",
        "destination": "/blog.html"
      },
      {
        "source": "/blog/article/**",
        "destination": "/blog/article/**.html",
        "dynamicLinks": true
      },
      {
        "source": "/blog/tag/**",
        "destination": "/blog/tag/**.html",
        "dynamicLinks": true
      },
      {
        "source": "/confirm/**",
        "destination": "/confirm/**.html",
        "dynamicLinks": true
      },
      {
        "source": "/confirm/forgot/**",
        "destination": "/confirm/forgot/**.html",
        "dynamicLinks": true
      }
    ]
  }
}
